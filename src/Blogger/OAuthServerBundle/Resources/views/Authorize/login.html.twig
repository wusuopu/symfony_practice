{{ form_start(form) }}
    <div class="form-group col-md-offset-4 col-md-4">
        {{ form_row(form.username, {'attr': {'class': 'form-control form-control-inverse'}}) }}
    </div>
    <div class="form-group col-md-offset-4 col-md-4">
        {{ form_row(form.password, {'attr': {'class': 'form-control form-control-inverse'}}) }}
    </div>
    <div class="form-group col-md-offset-4 col-md-4">
        {{ form_rest(form) }}
        <button type="submit" class="btn btn-dark btn-block" />登录</button>
    </div>
</form>
